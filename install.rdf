<?xml version="1.0" encoding="UTF-8"?>
<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#" xmlns:em="http://www.mozilla.org/2004/em-rdf#">
  <Description about="urn:mozilla:install-manifest">
    <!-- Unique ID for extension. Can be in e-mail address format or GUID format -->
    <em:id>deliciouspick@toniaddons.com</em:id>
    <!-- Indicates that this add-on is an extension -->
    <em:type>2</em:type>
    <!-- Extension name displayed in Add-on Manager -->
    <em:name>Delicious Pick</em:name>
    <!-- Extension version number. There is a version numbering scheme you must follow -->
    <em:version>1.0</em:version>
    <!-- Name of extension's primary developer. Change to your name -->
    <em:creator>Antonio Herraiz</em:creator>
    <!-- Optional. Contributor's name -->
    <em:contributor></em:contributor>
    <!-- Brief description displayed in Add-on Manager -->
    <em:description>Randomly pick one of your Delicious bookmarks.</em:description>
    <!-- Web page address through which extension is distributed -->
    <em:homepageURL>http://www.toniblogs.com/firefox-add-ons/</em:homepageURL>
    
    <!-- About dialog -->
    <!-- <em:aboutURL>chrome://deliciouspick/content/about.xul</em:aboutURL> -->
    <!-- About page can be a xul <dialog>, when commented out like this, it defaults to a generic dialog -->
    
    <!-- Extension preferences dialog -->
    <em:optionsURL>chrome://deliciouspick/content/options.xul</em:optionsURL>
    
    <!-- This section gives details of the target application for the extension -->
    <em:targetApplication>
      <Description>
        <em:id>{ec8030f7-c20a-464f-9b0e-13a3a9e97384}</em:id> <!-- Firefox -->
        <em:minVersion>3.6</em:minVersion>
        <em:maxVersion>4.0.*</em:maxVersion>
      </Description>
    </em:targetApplication>
  </Description>
</RDF>
